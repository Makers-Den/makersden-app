{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "APP_ENVIRONMENT",
    "GOOGLE_SHEETS_API_KEY",
    "NEXT_PUBLIC_EXAMPLE_ESTIMATION_SECRET",
    "NEXT_PUBLIC_STORYBLOK_ACCESS_TOKEN",
    "NODE_ENV",
    "PORT",
    "SLACK_WEBHOOK_URL",
    "STORYBLOK_ENVIRONMENT_FOLDER_NAME",
    "STORYBLOK_ESTIMATIONS_FOLDER_ID",
    "STORYBLOK_OAUTH_TOKEN",
    "STORYBLOK_PREVIEW_SECRET",
    "STORYBLOK_SPACE_ID",
    "VERCEL_URL",
    "DEVELOPMENT"
  ],
  "globalEnv": [
    "APP_ENVIRONMENT",
    "GOOGLE_SHEETS_API_KEY",
    "NEXT_PUBLIC_EXAMPLE_ESTIMATION_SECRET",
    "NEXT_PUBLIC_STORYBLOK_ACCESS_TOKEN",
    "NODE_ENV",
    "PORT",
    "SLACK_WEBHOOK_URL",
    "STORYBLOK_ENVIRONMENT_FOLDER_NAME",
    "STORYBLOK_ESTIMATIONS_FOLDER_ID",
    "STORYBLOK_OAUTH_TOKEN",
    "STORYBLOK_PREVIEW_SECRET",
    "STORYBLOK_SPACE_ID",
    "VERCEL_URL",
    "DEVELOPMENT"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "proxy": {
      "outputs": []
    },
    "ts:check": {
      "outputs": []
    }
  }
}
