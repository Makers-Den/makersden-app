{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "GOOGLE_SHEETS_API_KEY",
    "NEXT_PUBLIC_EXAMPLE_ESTIMATION_SECRET",
    "NODE_ENV",
    "PORT",
    "STORYBLOK_ACCESS_TOKEN",
    "STORYBLOK_ENVIRONMENT_FOLDER_NAME",
    "STORYBLOK_ESTIMATIONS_FOLDER_ID",
    "STORYBLOK_OAUTH_TOKEN",
    "STORYBLOK_SPACE_ID",
    "VERCEL_URL"
  ],
  "globalDependencies": [
    "GOOGLE_SHEETS_API_KEY",
    "NEXT_PUBLIC_EXAMPLE_ESTIMATION_SECRET",
    "NODE_ENV",
    "PORT",
    "STORYBLOK_ACCESS_TOKEN",
    "STORYBLOK_ENVIRONMENT_FOLDER_NAME",
    "STORYBLOK_ESTIMATIONS_FOLDER_ID",
    "STORYBLOK_OAUTH_TOKEN",
    "STORYBLOK_SPACE_ID",
    "VERCEL_URL"
  ]
}
