{
  "cli": {
    "version": ">= 3.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "staging": {
      "env": {
        "API_URL": "https://staging.app.makersden.io/api"
      },
      "ios": {
        "simulator": true
      },
      "distribution": "internal"
    },
    "staging-store": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://staging.app.makersden.io/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://app.makersden.io/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
