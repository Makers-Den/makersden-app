{
  "cli": {
    "version": ">= 3.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "staging": {
      "ios": {
        "simulator": true
      },
      "distribution": "internal",
      "channel": "staging"
    },
    "staging-store": {
      "autoIncrement": true,
      "channel": "staging-store"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
